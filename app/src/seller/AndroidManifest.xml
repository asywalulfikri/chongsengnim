<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    xmlns:tools="http://schemas.android.com/tools">


    <application
        android:name="sembako.sayunara.android.App"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme1"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:theme">


        <activity
            android:name="sembako.sayunara.product.editProduct.PostProductActivity"
            android:label=""
            android:windowSoftInputMode="stateHidden|adjustPan"/>

        <activity
            android:name="sembako.sayunara.user.ListUserActivity"
            android:label=""
            android:windowSoftInputMode="stateHidden|adjustPan"/>

        <activity
            android:name="sembako.sayunara.apk.ApkListActivity"
            android:label="Apk Sayunara"
            android:windowSoftInputMode="stateHidden|adjustPan"/>

    </application>



</manifest>